fea = csvread('fea.csv')
gnd = csvread('gnd.csv')
gnd = gnd';

numTrain = [10, 50, 100, 125, 150, 190, 250, 370];
forest_acc = [];

for i=numTrain
   trainFea = fea(1:i,:);
   trainLabel = gnd(1:i,:);
   testFea = fea(i+1:380,:);
   testLaba = gnd(i+1:380,:);
   m = forestTrain(trainFea, testLa);
   forestLabel = forestTest(m, testFea);
   acc = sum(forestLabel == testLabel) / 190;
   forest_acc = [forest_acc acc];

end
